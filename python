send-keys -t "5" C-z 'htop -u lblondel' Enter
send-keys -t "4" C-z 'ipython' Enter
send-keys -t "3" C-z 'vim' Enter
send-keys -t "1" C-z 'while true; do cdf ; sleep 10; clear ; done; ' Enter
