selectp -t 0              # Select pane 0
splitw -v -p 85 	  # Split pane 0 vertically by 50%
selectp -t 0              # Select pane 0
splitw -h -p 13
selectp -t 0              # Select pane 0
splitw -h -p 38 
selectp -t 3              # Select pane 1
splitw -h -p 30           # Split pane 1 horizontally by 25%
splitw -v -p 50 
